{
    "source": {
        "include": ["./src/"],
        "includePattern": ".+\\.js(doc)?$",
        "exclude": ["node_modules/"]
    },
    "opts": {
        "destination": "./docs/api/",
        "recurse": true
    },
    "plugins": [
        "plugins/markdown"
    ],
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false
    }
}
